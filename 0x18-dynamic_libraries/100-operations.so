#include <stdio.h>

int add(int a, int b) {
  return a + b;
}

int sub(int a, int b) {
  return a - b;
}

int mul(int a, int b) {
  return a * b;
}

int div(int a, int b) {
  return a / b;
}

int mod(int a, int b) {
  return a % b;
}

int main() {
  printf("add(10, 20) = %d\n", add(10, 20));
  printf("sub(10, 20) = %d\n", sub(10, 20));
  printf("mul(10, 20) = %d\n", mul(10, 20));
  printf("div(10, 20) = %d\n", div(10, 20));
  printf("mod(10, 20) = %d\n", mod(10, 20));
  return 0;
}
